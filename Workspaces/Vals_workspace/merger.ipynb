   "source": [
    "import pandas as pd\n",
    "import glob\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "file_paths = glob.glob('data/*.csv')\n",
    "dfs = []\n",
    "for file in file_paths:\n",
    "    df = pd.read_csv(file)\n",
    "    dfs.append(df)\n",
    "merged_df = pd.concat(dfs, ignore_index=True)\n",
    "merged_df.to_csv('data/merged_data.csv', index=False)"
   ]
  }
